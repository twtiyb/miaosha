{
  "name": "Tab Inspector",
  "description": "Utility for working with the extension tabs api",
  "version": "0.3",
  "permissions": ["tabs","https://*/*","http://*/*"],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "browser_action": {
    "default_title": "show tab inspector"
  },
  "manifest_version": 2
}
